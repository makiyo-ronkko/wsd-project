<!DOCTYPE html>
<html>
<head>
  <title><%= it.title %></title>
</head>
<body>
  <h1><%= it.title %></h1>
  <div>
  <p>How would you rate this experience?</p>
<form method="POST" action="/feedbacks/1">
  <button type="submit" name="value">Poor</button>
</form>
<form method="POST" action="/feedbacks/2">
  <button type="submit" name="value">Fair</button>
</form>
<form method="POST" action="/feedbacks/3">
  <button type="submit" name="value">Good</button>
</form>
<form method="POST" action="/feedbacks/4">
  <button type="submit" name="value">Very good</button>
</form>
<form method="POST" action="/feedbacks/5">
  <button type="submit" name="value">Excellent</button>
</form>
  </div>
  <p>Add a course:</p>
    <form method="POST" action="/courses">
      <label for="name">Course name:</label>
      <input type="text" id="name" name="name" /><br/>
      <input type="submit" value="Add" />
    </form>
    <p>Existing courses:</p>
    <ul>
    <% it.courses && it.courses.forEach((course) => { %>
      <li>
        <a href="/courses/<%= course.id %>"><%= course.name %></a>
        <form method="POST" action="/courses/<%= course.id %>/delete">
          <input type="submit" value="Delete" />
        </form>
      </li>
    <% }) %>
    </ul>
</body>